//ASCII equivalent
#include<stdio.h>
int main(){
    char ch;
    printf("Enter any character: ");
    scanf("%c",&ch);
    printf("ASCII equivalent of %c is %d",ch,ch);
    return 0;
}
//Quotient and reminder
int main(){
    int a,b,reminder;
    float quotient;
    printf("Enter a,b\n");
    scanf("%d%d",&a,&b);
    quotient=a/b;
    reminder=a%b;
    printf("The Quotient is=%f",quotient);
    printf("The Reminder is=%d",reminder);
    return 0;
}
//swapping of number without third varible
int main(){
    int x,y;
    printf("Enter the value of x & y:");
    scanf("%d%d",&x,&y);
    printf("before swapping the values are %d&%d\n",x,y);
    x=x+y;
    y=x-y;
    x=x-y;
    printf("Enter values after swapping is=%d&%d",x,y);
    return 0;
}
//swapping of number with third variable
int main(){
    int first,second,temp;
    printf("Enter the first &second number:");
    scanf("%d%d",&first,&second);
    temp=first;
    first=second;
    second=temp;
    printf("After swapping the values become %d %d",first,second);
    return 0;
}
//odd or even
int main(){
    int n;
    printf("Enter any number:");
    scanf("%d",&n);
    if(n%2==0)
    printf("The number is even");
    else
    printf("The number is odd");
    return 0;
}
//leap year
int main(){
    int year;
    printf("Enter year: ");
    scanf("%d",&year);
    if(year%4==0||year%400)
    printf("Year is a leap year");
    else
    printf("not a leap year");
    return 0;
}
//check positive,negative,whole
int main(){
    int n;
    printf("Enter a digit: ");
    scanf("%d",&n);
    if(n<0){
        if(n%2==0)
        printf("negative Even");
        else
        printf("negative Odd");
    }
    else if (n>0){
        if(n%2==0)
        printf("Positive Even");
        else
        printf("Positive Odd");
    }
    else if(n==0)
    printf("Whole number %d",n);
    else
    printf("Invalid");
    return 0;
}
//sum of n natural number
int main(){
    int n,i,sum=0;
    printf("Enter number: ");
    scanf("%d",&n);
    for(i=1;i<=n;i++){
        sum=sum+i;
    }
    printf("Sum is %d",sum);
    return 0;
}
//factorial of a number
int main(){
    int n,i,factorial=1;
    printf("Enter number: ");
    scanf("%d",&n);
    for(i=1;i<=n;i++){

        factorial=factorial*i;
    }
    printf("Factorial of %d=%d",n,factorial);
    return 0;
}
//table
int main(){
    int i,n;
    printf("Enter number: ");
    scanf("%d",&n);
    for(i=1;i<=10;i++){
        printf("%d\n",n*i);
    }
    return 0;
}
// facbonacci
int main(){
    int a=0,b=1,c,i,n;
    printf("Enter the number: ");
    scanf("%d",&n);
    printf("%d %d",a,b);
    for(i=2;i<=n;++i){
        c=a+b;
        printf("%d",c);
        a=b;
        b=c;
    }
return 0;
}
//all odd,even number
int main(){
    int n,min,max;
    printf("Enter the minimum range: ");
    scanf("%d",&min);
    printf("Enter the maximum: ");
    scanf("%d",&max);
    printf("Odd nos are:");
    for(n=min;n<=max;n++){
        if(n%2!=0)
        printf("%d,",n);
    }
    printf("\nEven nos are: ");
    for(n=min;n<=max;n++){
        if(n%2==0)
        printf("%d,",n);
    }
    return 0;
}
//GCD
int main(){
    int n1,n2;
    printf("Enter no1 and no2: ");
    scanf("%d%d",&n1,&n2);
    while(n1!=n2){
        if(n1>n2)
        n1-=n2;
        else
        n2-=n1;
    }
printf("GCD=%d",n1);
return 0;
}
//print a to z
int main(){
    char c;
    for(c='A';c<='Z';++c){
        printf("%c",c);  
        }
    return 0;
}
//sum of 3 digit number
int main(){
    int n,sum,a,b,d;
    printf("Enter a 3 digit no: ");
    scanf("%d",&n);
    a=n%10;
    n=n/10;
    b=n%10;
    n=n/10;
    d=n%10;
    sum=a+b+d;
    printf("%d",sum);
}
//reverse number
int main(){
    int n,c=0,a,b,d,rev;
    printf("Enter a no: ");
    scanf("%d",&n);
    a=n%10;
    n=n/10;
    b=n%10;
    n=n/10;
    d=n%10;
    n=n/10;
    rev=a*100+b*10+d*1;
    printf("Reversed no is %d",rev);
    return 0;
}
//power of number
#include<math.h>
int main(){
    int a,n,f;
    printf("Enter base & power: ");
    scanf("%d%d",&a,&n);
    f=pow(a,n);
    printf("%d raised to power %d is %d",a,n,f);
    return 0;
}
//pallindrome
int main(){
    int n,rev=0,rem=0,o;
    printf("Enter an integer: ");
    scanf("%d",&n);
    o=n;
    while(n!=0){
        rem=n%10;
        rev=rev*10+rem;
        n/=10;
    }
    if(o==rev)
    printf("%d is a Pallindrome",o);
    else
    printf("%d is not a pallindrome",o);
    return 0;
}
//prime or not
#include<math.h>
int main(){
    int n,i,m=0;
    printf("Enter a no.:");
    scanf("%d",&n);
    if(n==0||n==1)
    m=1;
    for(i=2;i<=n/2;++i){
        if(n%i==0)
        m=1;
    }
    if(m==0)
    printf("Prime");
    else
    printf("Composite");
    return 0;
}
// factors of number
int main(){
    int n,i;
    printf("Enter no.: ");
    scanf("%d",&n);
    printf("Factors are: ");
    for(i=1;i<=n;i++){
        if(n%i==0)
        printf("%d,",i);
    }
    return 0;
}
//calculator
int main(){
    int a,b;
    char op;
    printf("Enter operator(+,-,*,/): ");
    scanf("%c",&op);
    printf("Enter two operands: ");
    scanf("%d%d",&a,&b);
    switch(op){
        case'+':
        printf("%d+%d=%d",a,b,a+b);
        break;
        case'-':
        printf("%d-%d=%d",a,b,a-b);
        break;
        case'*':
        printf("%d*%d=%d",a,b,a*b);
        break;
        case'/':
        printf("%d/%d=%d",a,b,a/b);
        break;
        default:
        printf("Operator is invalid");
    }
    return 0;
}
//print 1 to n
int main(){
    int n,i=1;
    printf("Enter no.");
    scanf("%d",&n);
    while(i<=n){
        printf("%d\n",i);
        i++;
    }
    return 0;
}
//print 1 to 15
int main(){
    int i=1;
    do{
        printf("%d\n",i);
        i++;
    }
    while(i<=15);
    return 0;
}
//menu of resturant
int main(){
    int item,food,choice;
    printf("1=veg/2=non-veg:");
    scanf("%d",&item);
    switch(item){
        case 1:
        {
            printf("1=rice,2=dal,3=paneer,4=fries:");
            scanf("%d",&food);
            switch(food){
                case 1:
                printf("Rice@100");
                break;
                case 2:
                printf("Dal@60");
                break;
                case 3:
                printf("Paneer@160");
                break;
                case 4:
                printf("Fries@30");
                break;
            } break;
            case 2:
            {
                printf("5=fish curry,6=chicken curry,7=mutton curry: ");
                scanf("%d",&choice);
                switch(choice){
                    case 5:
                    printf("Fish curry@270");
                    break;
                    case 6:
                    printf("Chicken curry@300");
                    break;
                    case 7:
                    printf("Mutton curry @400");
                    break;
                }  
            }break;
            default:
            printf("Invalid choice");
        }
    }
    return 0;
}
// largest and smallest number
#include<stdio.h>
#include<math.h>
int main() {
    int a,b,c;
    printf("Enter 1st number: ");
    scanf("%d",&a);
    printf("Enter 2nd number: ");
    scanf("%d",&b);
    printf("Enter 3rd number: ");
    scanf("%d",&c);
    if (a>b&&a>c)
    printf("%d largest number\n",a);
    else if (b>a&&b>c)
    printf("%d largest number\n",b);
    else 
    printf("%d largest number\n",c);
    if(a<b&&a<c)
    printf("%d smallest number\n",a);
    else if (b<a&&b<c)
    printf("%d smallest number\n",b);
    else
    printf("%d smallest number\n",c);
    return 0;
}

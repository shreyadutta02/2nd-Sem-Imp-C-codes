    d=b*b-4*a*c;
    if(d>0){
        r1=(-b+sqrt(d))/(2*a);
        r2=(-b-sqrt(d))/(2*a);
        printf("Root1=%f&Root2=%f",r1,r2);
    }
    else if(d==0){
        r1=r2=-b/(2*a);
        printf("Root1=Root2=%f",r1);
    }
    else
    printf("Roots are imaginary");

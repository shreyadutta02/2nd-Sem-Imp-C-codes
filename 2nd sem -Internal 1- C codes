//sum of two numbers
#include<stdio.h>
int main(){
    int a,b;
    printf("Enter a");
    scanf("%d",&a);
    printf("Enter b");
    scanf("%d",&b);
    int sum=a+b;
    printf("Sum is %d",sum);
    return 0;
}
